{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/upload.js": {
      "maxDuration": 30
    },
    "api/ocr.js": {
      "maxDuration": 60
    }
  }
}